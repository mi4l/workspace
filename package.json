  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "oxlint": "^0.11.0",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "oxlint --max-warnings 0",
